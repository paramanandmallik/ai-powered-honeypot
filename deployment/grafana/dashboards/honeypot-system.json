{
  "dashboard": {
    "id": null,
    "title": "AI Honeypot System Dashboard",
    "tags": ["honeypot", "agentcore", "security"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"agentcore\"}",
            "legendFormat": "AgentCore Runtime"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Active Agents",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(up{job=\"agents\"})",
            "legendFormat": "Active Agents"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Honeypot Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(honeypot_interactions_total[5m])",
            "legendFormat": "Interactions/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Threat Detection Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(threats_detected_total[5m])",
            "legendFormat": "Threats/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Intelligence Reports Generated",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(intelligence_reports_total[5m])",
            "legendFormat": "Reports/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}