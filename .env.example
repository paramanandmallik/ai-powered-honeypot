# AI Honeypot System Configuration

# AgentCore Runtime Configuration
AGENTCORE_RUNTIME_ENDPOINT=https://agentcore.amazonaws.com
AGENTCORE_RUNTIME_REGION=us-east-1
AGENTCORE_RUNTIME_PROFILE=default

# AWS Configuration
AWS_PROFILE=default
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012

# AI Model Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Database Configuration
DATABASE_URL=sqlite:///data/honeypot.db
REDIS_URL=redis://localhost:6379/0

# Security Configuration
SECRET_KEY=your_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here
JWT_SECRET=your_jwt_secret_here

# System Configuration
MAX_CONCURRENT_ENGAGEMENTS=10
ENGAGEMENT_TIMEOUT_MINUTES=60
LOG_LEVEL=INFO
DEBUG_MODE=false

# Honeypot Configuration
WEB_HONEYPOT_PORT=8080
SSH_HONEYPOT_PORT=2222
DATABASE_HONEYPOT_PORT=3306
FILE_SHARE_HONEYPOT_PORT=445
EMAIL_HONEYPOT_PORT=25

# Monitoring Configuration
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
METRICS_ENABLED=true
HEALTH_CHECK_INTERVAL=30

# Development Configuration
DEVELOPMENT_MODE=true
MOCK_AGENTCORE=true
LOCAL_TESTING=true